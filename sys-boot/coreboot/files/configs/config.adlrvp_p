CONFIG_VENDOR_INTEL=y
CONFIG_BOARD_INTEL_ADLRVP_P=y

# SPI Descriptor
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/google/brya/descriptor-adlrvp_p.bin"

# FSP Blobs
CONFIG_DISPLAY_FSP_CALLS_AND_STATUS=y
CONFIG_DISPLAY_FSP_HEADER=y

# CSE data
CONFIG_CSE_DATA_FILE="cse_data/cse_data-adlrvp_p.bin"
CONFIG_CSE_FPT_FILE="es_inputs/csme.bin"
CONFIG_CSE_PMCP_FILE="es_inputs/pmc.bin"
CONFIG_CSE_IOMP_FILE="es_inputs/iom.bin"
CONFIG_CSE_TBTP_FILE="es_inputs/tbt.bin"
CONFIG_CSE_NPHY_FILE="es_inputs/nphy.bin"
CONFIG_CSE_PCHC_FILE="es_inputs/pchc.bin"
CONFIG_CSE_IUNP_FILE="es_inputs/iunit.bin"


# Video Blob
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/google/brya/vbt.bin"
CONFIG_INTEL_GMA_HAVE_VBT=y

# EC
CONFIG_HAVE_EC_BIN=y
CONFIG_EC_BIN_PATH="3rdparty/blobs/mainboard/google/brya/ec-adlrvp_p.bin"
CONFIG_ADL_INTEL_EC=y

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/google/brya/ucode-adlrvp_p.bin"
