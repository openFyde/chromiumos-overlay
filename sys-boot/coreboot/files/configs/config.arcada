CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_ARCADA=y

CONFIG_PAYLOAD_NONE=y
CONFIG_USE_BLOBS=y

# Chrome OS
CONFIG_CHROMEOS=y

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_EXTRA=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

CONFIG_SPI_FLASH_SMM=y

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=y
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/sarien/descriptor.bin"

# FSP blobs
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/mainboard/sarien/fsp/Include/"
CONFIG_FSP_M_FILE="3rdparty/blobs/mainboard/sarien/fsp/Fsp_M.fd"
CONFIG_FSP_S_FILE="3rdparty/blobs/mainboard/sarien/fsp/Fsp_S.fd"
CONFIG_ADD_FSP_BINARIES=y
CONFIG_INTEL_GMA_ADD_VBT=y
CONFIG_HAVE_FSP_GOP=y
CONFIG_RUN_FSP_GOP=y
CONFIG_FSP_M_XIP=y

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/sarien/ucode.bin"
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/sarien/vbt.bin"

# Management Engine
CONFIG_HAVE_ME_BIN=y
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/sarien/me.bin"

# EC
CONFIG_HAVE_EC_BIN=y
CONFIG_EC_BIN_PATH="3rdparty/blobs/mainboard/arcada-ec.bin"

# GbE
CONFIG_HAVE_GBE_BIN=y
CONFIG_GBE_BIN_PATH="3rdparty/blobs/mainboard/sarien/gbe.bin"
