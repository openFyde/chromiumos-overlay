CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_WINTERHOLD=y
CONFIG_CHROMEOS=y
CONFIG_PAYLOAD_NONE=y
# CONFIG_CONSOLE_SERIAL is not set
CONFIG_COLLECT_TIMESTAMPS=y

