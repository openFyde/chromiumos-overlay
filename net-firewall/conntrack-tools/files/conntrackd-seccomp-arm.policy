# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

ARM_set_tls: 1
_llseek: 1
access: 1
bind: 1
brk: 1
chdir: 1
clock_gettime64: 1
close: 1
connect: 1
execve: 1
exit: 1
exit_group: 1
fcntl64: 1
fstatfs64: 1
getpid: 1
getrandom: 1
getsockname: 1
getsockopt: 1
ioctl: arg1 == TCGETS
listen: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
openat: 1
pselect6: 1
read: 1
recvfrom: 1
recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
sched_get_priority_max: 1
sched_setscheduler: 1
send: 1
sendto: 1
set_robust_list: 1
set_tid_address: 1
setsockopt: 1
socket: arg0 == AF_NETLINK || arg0 == AF_UNIX
statx: 1
ugetrlimit: 1
umask: 1
uname: 1
unlink: 1
write: 1
