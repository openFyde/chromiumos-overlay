CONFIG_VENDOR_INTEL=y
CONFIG_BOARD_INTEL_GLKRVP=y
CONFIG_CBFS_SIZE=0x300000

CONFIG_PAYLOAD_NONE=y
CONFIG_RESET_ON_INVALID_RAMSTAGE_CACHE=y
CONFIG_SPI_FLASH=y
CONFIG_SPI_FLASH_SMM=y
CONFIG_USE_BLOBS=y
CONFIG_INCLUDE_NHLT_BLOBS=y

# Chrome OS
CONFIG_CHROMEOS=y
CONFIG_SEPARATE_VERSTAGE=y
CONFIG_VBOOT_STARTS_IN_BOOTBLOCK=y

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_COLLECT_TIMESTAMPS=y
CONFIG_ELOG=y
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

# Firmware Support Package
CONFIG_ADD_FSP_BINARIES=y
CONFIG_HAVE_INTEL_FSP_REPO=y
# CONFIG_FSP_USE_REPO is not set
CONFIG_FSP_M_FILE="3rdparty/blobs/mainboard/intel/glkrvp/fsp/FSP-M.Fv"
CONFIG_FSP_S_FILE="3rdparty/blobs/mainboard/intel/glkrvp/fsp/FSP-S.Fv"
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/mainboard/intel/glkrvp/fsp/Include/"
CONFIG_INTEL_GMA_ADD_VBT=y
CONFIG_RUN_FSP_GOP=y
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/intel/glkrvp/vbt.bin"

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=y
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/intel/glkrvp/descriptor.bin"

# Management Engine
# CONFIG_LOCK_MANAGEMENT_ENGINE is not set

# IFWI
CONFIG_NEED_IFWI=y
CONFIG_IFWI_FMAP_NAME="IFWI"
CONFIG_IFWI_FILE_NAME="3rdparty/blobs/mainboard/intel/glkrvp/fitimage.bin"

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/intel/glkrvp/ucode.bin"

# CONFIG_CONSOLE_SERIAL is not set
CONFIG_CPU_MICROCODE_CBFS_EXTERNAL_BINS=y
