CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_ZYDRON=y

# SPI Descriptor
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/google/brya/descriptor-zydron.bin"

# CSE data
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/google/brya/csme-zydron.bin"
CONFIG_SOC_INTEL_CSE_RW_FILE="3rdparty/blobs/mainboard/google/brya/me_rw-zydron.bin"
CONFIG_SOC_INTEL_CSE_RW_UPDATE=y
CONFIG_SOC_INTEL_CSE_RW_VERSION="16.1.25.2027"

# Use RPL FSP Blobs
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/intel/rpl/fsp/Include"
CONFIG_FSP_M_FILE="3rdparty/blobs/intel/rpl/fsp/fspm.bin"
CONFIG_FSP_S_FILE="3rdparty/blobs/intel/rpl/fsp/fsps.bin"

# Video Blob
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/google/brya/vbt.bin"

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/google/brya/ucode-zydron.bin"

# Wifi SAR
CONFIG_CHROMEOS_WIFI_SAR=y
