# udev rules to start firmware update for NVME SSD: SSSTC
SUBSYSTEM=="nvme", \
ATTRS{vendor}=="0x1e95", \
ATTR{firmware_rev}!="CR22002", \
ATTR{model}=="CL1-3D256", \
ACTION=="add", \
RUN+="/sbin/initctl emit fwupdtool-update PLUGIN=nvme GUID=da6c6856-9c1f-5f3d-98fc-5d2179f21b18"
